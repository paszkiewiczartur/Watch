<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<BorderPane prefHeight="300.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.StopwatchPaneController">
   <right>
      <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
         <children>
            <TableView fx:id="StopwatchTableView" maxHeight="1.7976931348623157E308" prefHeight="239.0" prefWidth="100.0">
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
            <Button fx:id="StopwatchResetButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Reset" />
         </children>
      </VBox>
   </right>
   <bottom>
      <HBox prefHeight="50.0" prefWidth="352.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="StopwatchClearButton" maxHeight="1.7976931348623157E308" mnemonicParsing="false" text="Clear">
               <HBox.margin>
                  <Insets left="50.0" right="30.0" />
               </HBox.margin>
            </Button>
            <ToggleButton fx:id="StopwatchStartButton" maxHeight="1.7976931348623157E308" maxWidth="59.0" minWidth="59.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="59.0" text="Start" HBox.hgrow="NEVER">
               <HBox.margin>
                  <Insets right="30.0" />
               </HBox.margin>
            </ToggleButton>
            <Button fx:id="StopwatchMiddleTimeButton" maxHeight="1.7976931348623157E308" mnemonicParsing="false" text="Lap time" />
         </children>
      </HBox>
   </bottom>
   <center>
      <TextField fx:id="StopwatchTextField" editable="false" maxWidth="-Infinity" prefHeight="63.0" prefWidth="246.0" BorderPane.alignment="CENTER">
         <padding>
            <Insets left="30.0" right="30.0" />
         </padding>
      </TextField>
   </center>
</BorderPane>
